                    android:textSize="12dp"/>

            </LinearLayout>
            <LinearLayout
                android:paddingStart="20dp"
                android:paddingVertical="15dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:textColor="@color/black"
                    android:layout_width="match_parent"
